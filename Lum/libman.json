{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "bulma@1.0.0",
      "destination": "wwwroot/lib/bulma",
      "files": [
        "css/**/*.min.css"
      ]
    },
    {
      "library": "@fortawesome/fontawesome-free@6.5.2",
      "destination": "wwwroot/lib/fontawesome-free",
      "files": [
        "css/all.min.css",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-solid-900.woff2"
      ]
    }
  ]
}